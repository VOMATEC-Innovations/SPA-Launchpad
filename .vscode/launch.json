{
    // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
    // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
    // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js",
            "env": {
                "SPA_apiBasePath": "https://api.product.cloud",
                "SPA_authEndpoint": "https://id.product.cloud/realms/product/auth",
                "SPA_tokenEndpoint": "https://id.product.cloud/realms/product/token",
                "SPA_provider": "https://id.product.cloud/realms/product/protocols/openid-connect"
            }
        }
    ]
}